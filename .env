# .env
# Use app-specific user, not the postgres superuser.
DATABASE_URL=postgresql+psycopg://permit_app:${PERMIT_DB_PASSWORD}@containers-us-west-146.railway.app:6543/railway?sslmode=require

# Set this in Railway as a secret (Environment -> Variables)
PERMIT_DB_PASSWORD=CHANGE_ME_STRONG

# Scraper configuration
SCRAPE_BEGIN_DATE=09/23/2025
SCRAPE_END_DATE=09/23/2025
SCRAPE_MAX_PAGES=10

# Railway environment detection
RAILWAY_ENVIRONMENT=development
